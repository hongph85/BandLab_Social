{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "ImageBlobStorage": {
    "ORIGIN_CONTAINER_NAME": "origin",
    "AzureWebJobsStorage": "[Blob connection string]",
    "StorageEndpoint": "[Blob Endpoint]"
  },
  "CosmosDB": {
    "Endpoint": "https://localhost:8081",
    "DatabaseId": "BandlabDB",
    "SocialContainer": "Posts",
    "AuthToken": "[Cosmos Token]"
  }
}
